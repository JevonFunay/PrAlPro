    "#Uang Awal 200.000.000\n",
    "#Bunga 10%\n",
    "\n",
    "UA = 200000000\n",
    "B = 10/100\n",
    "TU = UA\n",
    "THN = 0\n",
    "\n",
    "while TU<= 400000000:\n",
    "    THN=THN+1\n",
    "    TU = float(TU*(1+B))\n",
    "X = TU\n",
    "print(\"Perlu waktu =\", THN, \"Agar Rp. 200.000.000 JT menjadi Rp =\", X)"
